{
    "manifest_version": 3,
    "name": "Filtro Google Lens",
    "version": "1.9.0",
    "description": "Filtrado de competidores dentro de los resultados de Google Lens",
    "icons": {
        "48": "images/icon-32.png",
        "128": "images/icon-128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "activeTab",
        "storage",
        "webRequest",
        "tabs",
        "commands"
    ],
    "host_permissions": [
        "https://lens.google.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://lens.google.com/*"
            ],
            "js": [
                "content.js"
            ]
        },
        {
            "matches": [
                "https://m.shein.com.mx/*"
            ],
            "js": [
                "scripts/sheinmx.js"
            ]
        }
    ],
    "commands": {
        "close-other-tabs": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y"
            },
            "description": "Cerrar todas las pesta√±as excepto la actual"
        }
    }
}